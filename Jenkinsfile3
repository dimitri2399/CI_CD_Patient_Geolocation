pipeline{
    agent {
        label 'ansible'
    }
    stages{
        stage('check the current dictory'){
        steps{
            sh 'pwd'
            sh 'whoami'
            sh "mkdir \$(date '+%Y%m%d_%H%M%S')"
            sh 'python3 --version'
            sh 'npm --version'
        }
    }    
}
}